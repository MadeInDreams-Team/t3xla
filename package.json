{
  "name": "@madeindreams-team/t3xla",
  "version": "0.1.0",
  "description": "A Low Carbon Footprint Cryptocurency. Validating nodes can run on a solar powered Raspberry Pi",
  "main": "build/main.js",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "base-64": "^1.0.0",
    "bip39": "^3.0.3",
    "crypto-js": "^4.0.0",
    "ethers": "^5.1.4",
    "i18next": "^20.2.4",
    "i18next-browser-languagedetector": "^6.1.1",
    "i18next-http-backend": "^1.2.4",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.0",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.15",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "reselect": "^4.0.0",
    "secp256k1": "^4.0.2",
    "web-vitals": "^1.0.1"
  },
  "build": {
    "appId": "t3xla",
    "extends": null,
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "icon": "build/logo.ico"
    }
  },
  "scripts": {
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadeInDreams-Team/t3xla.git"
  },
  "keywords": [
    "Low",
    "Carbon",
    "Foortprint",
    "Cryptocurrency"
  ],
  "author": "Ian Decentralize",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MadeInDreams-Team/t3xla/issues"
  },
  "devDependencies": {
    "electron": "^12.0.7",
    "electron-builder": "^22.11.4"
  }
}
